{
  "presets": ["env", "react", "stage-2"],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    ["transform-runtime", { "polyfill": false, "regenerator": true }],
    [
      "babel-plugin-root-import",
      [
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "src"
        }
      ]
    ]
  ]
}
